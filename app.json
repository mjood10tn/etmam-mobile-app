{
  "expo": {
    "name": "إتمام",
    "slug": "etmam-mobile-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logos/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F9FA"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logos/adaptive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#F8F9FA"
      },
      "package": "com.mjood10tn.etmammobileapp"
    },
    
    "ios": {
      "bundleIdentifier": "com.mjood10tn.etmammobileapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "812e23f8-9414-4725-900e-5b2a8bf6be2d"
      }
    }
  }
}
